{
    "name": "swiftly/config",
    "description": "Simple config management utilities.",
    "homepage": "https://github.com/SwiftlyPHP/config",
    "readme": "README.md",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "symfony/polyfill-php84": "^1.33"
    },
    "require-dev": {
        "vimeo/psalm": "^6.14",
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.92"
    },
    "autoload": {
        "psr-4": {
            "Swiftly\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swiftly\\Config\\Tests\\": "tests/"
        }
    }
}
